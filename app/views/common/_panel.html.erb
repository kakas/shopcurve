<% id = title.gsub(/\s/, "") %>
<div class="panel">
  <div class="panel-heading">
    <a class="panel-title" data-toggle="collapse" href="#<%= id %>">
      <%= title %>
    </a>
  </div>
  <div id="<%= id %>" class="panel-collapse collapse in" role="tabpanel">
    <div class="panel-body text-gray">
      <%= yield %>
    </div>
  </div>
</div>
